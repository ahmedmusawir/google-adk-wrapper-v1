    {
      "parameters": {
        "jsCode": "// The new, simpler code. No more parsing!\n\n// Get the structured data directly from the webhook body.\nconst agentName = items[0].json.body.agent_name;\nconst message = items[0].json.body.message;\nconst userId = items[0].json.body.userId;\n\n// Prepare the clean output for the next node (HTTP Request).\nconst newOutput = {\n  agent_name: agentName,\n  message: message,\n  user_id: userId\n};\n\n// Replace the original data with our clean data.\nitems[0].json = newOutput;\n\n// Return the result for the next node.\nreturn items;"
      },
      "type": "n8n-nodes-base.code",
